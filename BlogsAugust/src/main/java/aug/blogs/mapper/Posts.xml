<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aug.blogs.dao.PostsMapper">

	<resultMap type="Posts" id="PostsResult">
		<id property="id" column="id"/>
		<result property="author" column="author"/>
		<result property="postDate" column="post_date"/>
		<result property="postModified" column="post_modified"/>
		<result property="contents" column="contents"/>
		<result property="title" column="title"/>
		<result property="excerpt" column="excerpt"/>
		<result property="status" column="status"/>
		<result property="postName" column="post_name"/>
		<result property="guid" column="guid"/>
		<result property="termsId" column="terms_id"/>
		<result property="postType" column="post_type"/>
	</resultMap>

</mapper>